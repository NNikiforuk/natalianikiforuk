	xmlns:xlink="http://www.w3.org/1999/xlink"
	viewBox="0 0 170.13 142.24"
>
	<defs>
		<linearGradient
			id={gradientId}
			x1="0"
			y1="71.12"
			x2="170.13"
			y2="71.12"
			gradientUnits="userSpaceOnUse"
		>
			<stop offset="0" stop-color="#f7b9d4"></stop>
			<stop offset=".11" stop-color="#ecb9d4"></stop>
			<stop offset=".31" stop-color="#d1bbd5"></stop>
			<stop offset=".56" stop-color="#a4bed6"></stop>
			<stop offset=".86" stop-color="#67c3d8"></stop>
			<stop offset=".99" stop-color="#4bc5d9"></stop>
		</linearGradient>
	</defs>
	<path
		fill={`url(#${gradientId})`}
		d="M.97,85.49C-6.59,50.01,31.43,4.91,70.12.38c58.06-6.79,114.19,78.23,96.81,114.89-15.63,32.98-95.67,37.23-140.43,4.26-7.62-5.61-21.76-16.34-25.53-34.04Z"
