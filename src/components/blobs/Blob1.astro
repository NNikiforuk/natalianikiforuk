	xmlns="http://www.w3.org/2000/svg"
	xmlns:xlink="http://www.w3.org/1999/xlink"
	viewBox="0 0 344.35 260.67"
>
	<defs>
		<linearGradient
			id={gradientId}
			x1="130.57"
			y1="180.47"
			x2="406.8"
			y2="180.47"
			gradientTransform="translate(-113.23 284.74) rotate(-61.16)"
			gradientUnits="userSpaceOnUse"
		>
			<stop offset="0" stop-color="#f7b9d4"></stop>
			<stop offset=".11" stop-color="#ecb9d4"></stop>
			<stop offset=".31" stop-color="#d1bbd5"></stop>
			<stop offset=".56" stop-color="#a4bed6"></stop>
			<stop offset=".86" stop-color="#67c3d8"></stop>
			<stop offset=".99" stop-color="#4bc5d9"></stop>
		</linearGradient>
	</defs>
	<g>
		<path
			fill={`url(#${gradientId})`}
			d="M158.03,260.31c78.54,4.86,176.38-39.01,185.6-105.46C353.91,80.76,252.24-8.54,154.07.66,72.09,8.33-11.03,85.03,1.2,156.94c11.18,65.75,97.36,99.69,156.82,103.37Z"
